{
  "buildCommand": "node vercel-build.js",
  "outputDirectory": "build/client",
  "installCommand": "pnpm install",
  "framework": "remix",
  "functions": {
    "functions/[[path]].ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NODE_ENV": "production",
    "DISABLE_WORKERD": "true"
  }
}